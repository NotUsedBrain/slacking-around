
<% @job_applications.each do |post|%>
  <div class='container'>
    <%= 'Opening for '%>
    <strong> <%= link_to post.job_posting.title %> </strong>
    <%= 'at '%>
    <strong> <%= post.job_posting.title %> </strong>
    <%= 'starting at: '%>
    <%= post.job_posting.start_time%>
    <%= link_to 'Delete', post, method: :delete, data: {confirm: 'Are you sure?'} %>
  </div>
<% end %>